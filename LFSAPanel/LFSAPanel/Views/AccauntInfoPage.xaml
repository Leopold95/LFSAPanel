<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LFSAPanel.Views.AccauntInfoPage"
             xmlns:local ="clr-namespace:LFSAPanel.ViewModels">
    <ContentPage.BindingContext>
        <local:AccauntInfoPageViewModel/>
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding AccauntId}" HorizontalOptions="Center"/>
            <Label Text="{Binding IsAndmin}" HorizontalOptions="Center"/>
            <Label Text="{Binding UserName}" HorizontalOptions="Center"/>
            <Label Text="{Binding Email}" HorizontalOptions="Center"/>
            <Label Text="{Binding FirstName}" HorizontalOptions="Center"/>
            <Label Text="{Binding LastName}" HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>